{
  "name": "flavorcss",
  "version": "0.4.1",
  "description": "full build css",
  "main": "es/index.js",
  "types": "lib/index.ts",
  "private": false,
  "scripts": {
    "dev":"nodemon yarn es",
    "es": "esbuild --define:process.env.NODE_ENV=\\\"production\\\" lib/index.ts --outdir=es --bundle --format=esm --jsx-factory=aoife --jsx-fragment=aoife.Frag --minify --splitting",
    "build": "yarn esbuild && cp -rf es document/public/",
    "lib": "node rollup.config.js"
  },
  "devDependencies": {
    "esbuild": "^0.8.40",
    "fs-extra": "^9.0.0",
    "rollup": "^1.24.0",
    "rollup-plugin-typescript2": "^0.24.3",
    "rollup-plugin-uglify": "^6.0.3",
    "string.prototype.replaceall": "^1.0.4",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ymzuiku/flavorcss.git"
  },
  "bugs": {
    "url": "git+https://github.com/ymzuiku/flavorcss.git"
  },
  "homepage": "git+https://github.com/ymzuiku/flavorcss.git"
}
