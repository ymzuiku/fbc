{
  "name": "flavorcss",
  "version": "0.4.8",
  "description": "full build css",
  "keywords": ["ui", "css", "atmoic css"],
  "main": "es/index.js",
  "module": "es/index.js",
  "types": "lib/index.ts",
  "exports":{"import":"./es/index.js"},
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "esbuild --define:process.env.NODE_ENV=\\\"production\\\" lib/index.ts --outdir=es --bundle --format=esm --jsx-factory=aoife --jsx-fragment=aoife.Frag --minify --splitting",
    "es": "yarn build && cp -rf es/index.js document/public/flavorcss.js",
    "lib": "node rollup.config.js"
  },
  "devDependencies": {
    "esbuild": "^0.8.40",
    "fs-extra": "^9.0.0",
    "rollup": "^1.24.0",
    "rollup-plugin-typescript2": "^0.24.3",
    "rollup-plugin-uglify": "^6.0.3",
    "string.prototype.replaceall": "^1.0.4",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ymzuiku/flavorcss.git"
  },
  "bugs": {
    "url": "git+https://github.com/ymzuiku/flavorcss.git"
  },
  "homepage": "https://flavor.writeflowy.com"
}
